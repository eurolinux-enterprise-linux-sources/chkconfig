#: ../chkconfig.c:42
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "käyttö:   %s [--list] [--type <tyyppi>] [nimi]\n"

#: ../chkconfig.c:43
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nimi>\n"

#: ../chkconfig.c:44
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <nimi>\n"

#: ../chkconfig.c:45
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s --override <nimi>\n"

#: ../chkconfig.c:46
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <tasot>] [--type <tyyppi>] <nimi> %s\n"

#: ../chkconfig.c:53
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"palvelulla %s on chkconfig-tuki, mutta siihen ei viitata millään ajotasolla "
"(aja \"chkconfig --add %s\")\n"

#: ../chkconfig.c:55
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "palvelulla %s ei ole chkconfig-tukea\n"

#: ../chkconfig.c:57
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "virhe luettaessa palvelun %s tietoja: %s\n"

#: ../chkconfig.c:66
#, c-format
msgid "You do not have enough privileges to perform this operation.\n"
msgstr "Sinulla ei ole riittävästi oikeuksia tämän toiminnon suorittamiseen.\n"

#: ../chkconfig.c:379 ../chkconfig.c:384 ../chkconfig.c:498
msgid "on"
msgstr "on"

#: ../chkconfig.c:379 ../chkconfig.c:384 ../chkconfig.c:498
msgid "off"
msgstr "ei"

#: ../chkconfig.c:460
#, c-format
msgid "xinetd based services:\n"
msgstr "xinetd-pohjaiset palvelut:\n"

