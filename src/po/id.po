#: ../chkconfig.c:45
#, c-format
msgid "usage:   %s [name]\n"
msgstr "pemakaian:  %s [name]\n"

#: ../chkconfig.c:47
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "pemakaian :  %s [--list] [--type <type>] [name]\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s --add <name>\n"
msgstr "          %s --add <name>\n"

#: ../chkconfig.c:49
#, c-format
msgid "         %s --del <name>\n"
msgstr "           %s --del <name>\n"

