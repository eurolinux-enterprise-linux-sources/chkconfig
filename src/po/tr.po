#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "%s servisi chkconfig'i desteklemiyor\n"

#: ../chkconfig.c:57
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "%s servisi hakkında bilgi alınırken hata: %s\n"

