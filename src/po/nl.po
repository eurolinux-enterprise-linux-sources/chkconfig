#: ../chkconfig.c:45
#, c-format
msgid "usage:   %s [name]\n"
msgstr "gebruik:   %s [name]\n"

#: ../chkconfig.c:47
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "gebruik:   %s [--list] [--type <type>] [naam]\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <naam>\n"

#: ../chkconfig.c:49
#, c-format
