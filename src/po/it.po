#: ../chkconfig.c:45
#, c-format
msgid "usage:   %s [name]\n"
msgstr "utilizzo: %s [nome]⏎\n"

#: ../chkconfig.c:47
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "uso:   %s [--list] [--type·<tipo>] [nome]\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <nome>\n"

#: ../chkconfig.c:49
#, c-format
