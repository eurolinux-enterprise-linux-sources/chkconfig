#: ../chkconfig.c:42
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "upotreba: %s [--list] [--type <vrsta>] [ime]\n"

#: ../chkconfig.c:43
#, c-format
msgid "         %s --add <name>\n"
msgstr "          %s --add <ime>\n"

#: ../chkconfig.c:44
#, c-format
msgid "         %s --del <name>\n"
msgstr "          %s --del <ime>\n"

#: ../chkconfig.c:45
#, c-format
msgid "         %s --override <name>\n"
msgstr "          %s --override <ime>\n"

#: ../chkconfig.c:46
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "          %s [--level <nivoi>] [--type <vrsta>] <ime> %s\n"

#: ../chkconfig.c:53
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"servis %s podržava chkconfig, ali nije poznat ni u jednom nivou izvršavanja "
"(izvršite „chkconfig --add %s“)\n"

#: ../chkconfig.c:55
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "servis %s ne podržava chkconfig\n"

#: ../chkconfig.c:57
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "greška pri čitanju podataka o servisu %s: %s\n"

#: ../chkconfig.c:66
#, c-format
msgid "You do not have enough privileges to perform this operation.\n"
msgstr "Nemate dovoljne povlastice za izvršavanje ove operacije.\n"

#: ../chkconfig.c:357 ../chkconfig.c:362 ../chkconfig.c:476
msgid "on"
msgstr "uključeno"

#: ../chkconfig.c:357 ../chkconfig.c:362 ../chkconfig.c:476
msgid "off"
msgstr "isključeno"

