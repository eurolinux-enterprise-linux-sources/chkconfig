#: ../chkconfig.c:45
#, c-format
msgid "usage:   %s [name]\n"
msgstr "વપરાશ:   %s [name]\n"

#: ../chkconfig.c:47
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "વપરાશ:   %s [--list] [--type <type>] [name]\n"

