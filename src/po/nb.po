#: ../chkconfig.c:45
#, c-format
msgid "usage:   %s [name]\n"
msgstr "bruk: %s [navn]\n"

#: ../chkconfig.c:47
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "bruk:    %s [--list] [--type <type>] [navn]\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <navn>\n"

#: ../chkconfig.c:49
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <navn>\n"

#: ../chkconfig.c:50
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s --override <navn>\n"

#: ../chkconfig.c:51
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <nivåer>] [--type <type>] <navn> %s\n"

#: ../chkconfig.c:59
#, c-format
msgid ""
"\n"
"Note: This output shows SysV services only and does not include native\n"
"      systemd services. SysV configuration data might be overridden by native\n"
"      systemd configuration.\n"
"\n"
msgstr "\nMerk: Her vises kun SysV-tjenester - ikke systemd-tjenester\n      SysV-konfigurasjonsdata kan bli overskrevet av systemd-konfigurasjon.\n\n"

#: ../chkconfig.c:63
#, c-format
msgid ""
"      If you want to list systemd services use 'systemctl list-unit-files'.\n"
"      To see services enabled on particular target use\n"
"      'systemctl list-dependencies [target]'.\n"
"\n"
msgstr ""

#: ../chkconfig.c:71
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr "tjenesten %s støtter chkconfig men er ikke nevnt i noe kjøringsnivå (kjør 'chkconfig --add %s')\n"

