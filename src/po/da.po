#: ../chkconfig.c:45
#, c-format
msgid "usage:   %s [name]\n"
msgstr "brug:   %s [navn]\n"

#: ../chkconfig.c:47
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "brug:   %s [--list] [--type <type>] [navn]\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <navn>\n"

