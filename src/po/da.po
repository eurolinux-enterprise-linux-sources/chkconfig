#: ../chkconfig.c:42
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "brug:   %s [--list] [--type <type>] [navn]\n"

#: ../chkconfig.c:43
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --add <navn>\n"

#: ../chkconfig.c:44
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --del <navn>\n"

#: ../chkconfig.c:45
#, c-format
msgid "         %s --override <name>\n"
msgstr "         %s --override <navn>\n"

#: ../chkconfig.c:46
#, c-format
msgid "         %s [--level <levels>] [--type <type>] <name> %s\n"
msgstr "         %s [--level <niveau>] [--type <type>] <navn> %s\n"

#: ../chkconfig.c:53
#, c-format
msgid ""
"service %s supports chkconfig, but is not referenced in any runlevel (run "
"'chkconfig --add %s')\n"
msgstr ""
"tjenesten %s understøtter chkconfig, men refereres ikke fra noget "
"kørselsniveau (kør \"chkconfig --add %s\")\n"

#: ../chkconfig.c:55
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "tjeneste %s understøtter ikke chkconfig\n"

#: ../chkconfig.c:57
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "fejl ved læsning af information om tjeneste %s: %s\n"

#: ../chkconfig.c:66
#, c-format
msgid "You do not have enough privileges to perform this operation.\n"
msgstr "Du har ikke nok rettigheder til at udføre denne operation.\n"

#: ../chkconfig.c:379 ../chkconfig.c:384 ../chkconfig.c:498
msgid "on"
msgstr "til"

#: ../chkconfig.c:379 ../chkconfig.c:384 ../chkconfig.c:498
msgid "off"
msgstr "fra"

#: ../chkconfig.c:460
#, c-format
msgid "xinetd based services:\n"
msgstr "xinetd baserede tjenester:\n"

#: ../chkconfig.c:462
#, c-format
msgid "failed to open directory %s: %s\n"
msgstr "kunne ikke åbne katalog %s: %s\n"

#: ../chkconfig.c:606
#, c-format
msgid "%s version %s\n"
msgstr "%s version %s\n"

#: ../chkconfig.c:618
#, c-format
msgid "--type must be 'sysv' or 'xinetd'\n"
msgstr "--type skal være \"sysv\" eller \"xinetd\"\n"

#: ../chkconfig.c:626
#, c-format
msgid "only one of --list, --add, --del, or --override may be specified\n"
msgstr "kun én af --list, --add eller --del eller --override må angives\n"

#: ../chkconfig.c:686
#, c-format
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "kun ét kørselsniveau må angives for en chkconfig forespørgsel\n"

#: ../ntsysv.c:46
msgid "Press <F1> for more information on a service."
msgstr "Tast <F1> for mere information om en tjeneste."

#: ../ntsysv.c:83 ../ntsysv.c:120
msgid "Ok"
msgstr "O.k."

#: ../ntsysv.c:83
msgid "Back"
msgstr "Tilbage"

#: ../ntsysv.c:83
msgid "Cancel"
msgstr "Annullér"

#: ../ntsysv.c:95
msgid "What services should be automatically started?"
msgstr "Hvilke tjenester skal startes automatisk?"

#: ../ntsysv.c:101
msgid "Services"
msgstr "Tjenester"

#: ../ntsysv.c:183 ../ntsysv.c:210 ../ntsysv.c:248
