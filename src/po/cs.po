#: ../chkconfig.c:42
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "použití: %s [--list] [--type <typ>] [jméno]\n"

#: ../chkconfig.c:43
#, c-format
msgid "         %s --add <name>\n"
