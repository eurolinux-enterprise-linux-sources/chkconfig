#: ../chkconfig.c:411 ../chkconfig.c:416 ../chkconfig.c:530
msgid "on"
msgstr "활성"

#: ../chkconfig.c:411 ../chkconfig.c:416 ../chkconfig.c:530
msgid "off"
msgstr "해제"

