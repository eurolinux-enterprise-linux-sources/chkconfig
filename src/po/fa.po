#: ../chkconfig.c:45
#, c-format
msgid "usage:   %s [name]\n"
msgstr ""

#: ../chkconfig.c:47
#, c-format
msgid "usage:   %s [--list] [--type <type>] [name]\n"
msgstr "کاربرد:   %s [--list] [--type <type>] [name]\n"

#: ../chkconfig.c:48
#, c-format
msgid "         %s --add <name>\n"
msgstr "         %s --افزودن <name>\n"

#: ../chkconfig.c:49
#, c-format
msgid "         %s --del <name>\n"
msgstr "         %s --حذف <name>\n"

